
#include <p18F45K20.h>

#include <delays.h>

void main (void)

{


TRISA = 0b00000000; //sets PORTA as all outputs

LATA = 1; //turns off PORTA outputs 



while (1) 
{ 

LATAbits.RA1 = 1; 

Delay1KTCYx(78);
 
LATAbits.RA1 = 0;
 
Delay1KTCYx(78);

LATAbits.RA2 = 1; 

Delay1KTCYx(78);
 
LATAbits.RA2 = 0;
 
Delay1KTCYx(78);

LATAbits.RA3 = 1; 

Delay1KTCYx(78);
 
LATAbits.RA3 = 0;
 
Delay1KTCYx(78);

}
 
 } 

